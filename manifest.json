{
  "name": "AutoLogin",
  "description": "Automatically login to websites",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "version": "0.1",
  "action": {
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["sites.js", "content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3
}
