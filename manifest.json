{
  "name": "AutoLogin",
  "description": "Automatically login to websites",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["sites.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
